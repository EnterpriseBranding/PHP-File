    "keywords": [
        "File",
        "Search words in file",
        "File handler",
        "Strings",
        "HHVM",
        "PHP"
    ],
    "license": "MIT",
    "authors": [
        {
            "name":     "Josantonius",
            "email":    "info@josantonius.com",
            "homepage": "https://github.com/josantonius",
            "role":     "Developer"
        }
